{
    "name": "graham-campbell/security",
    "description": "Security Is A Port Of The Security Class From Codeigniter 3 For Laravel 5",
    "keywords": ["laravel", "framework", "security", "codeigniter", "Security", "Laravel Security", "Laravel-Security", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Security\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Security\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.7-dev"
        },
        "laravel": {
            "providers": [
                "GrahamCampbell\\Security\\SecurityServiceProvider"
            ],
            "aliases": {
                "Security": "GrahamCampbell\\Security\\Facades\\Security"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
